<header id="top-header">
    <span class="siteName">
      <a href="/" title="Return to home page">CSE Motors</a>
    </span>
    <div id="tools">
        <% if (locals.loggedin) { %>
            <div id="tools">
                <a href="/account/management">Welcome <%= locals.accountData.account_firstname %></a><br>
                <a href="/account/logout">Logout</a>
            </div>
        <% } else { %>
            <div id="tools">
                <a title="Click to log in" href="/account/login">My Account</a>
            </div>
        <% } %>
    </div>

  </header>


